{
	/* See all the pre-defined configs here: https://www.npmjs.com/package/eslint-config-defaults */
	"extends": [
		"eslint:recommended"
	],
	"env": {
		"amd": true,
		"browser": true,
		"jquery": true,
		"node": true,
		"es6": true,
		"worker": true
	},
	"rules": {
		"eqeqeq": 2,
		"comma-dangle": 1,
		"no-console": 0,
		"no-debugger": 0,
		"no-extra-semi": 1,
		"no-extra-parens": 1,
		"no-irregular-whitespace": 1,
		"no-undef": 0,
		"no-unused-vars": 1,
		"semi": 1,
		"semi-spacing": 1,
		"valid-jsdoc": [
			2,
			{
				"requireReturn": false
			}
		]
	}
}